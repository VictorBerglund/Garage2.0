@model IEnumerable<Garage2._0.Models.Garage>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Park", "Park")

   @using (Html.BeginForm("Index", "Garage", FormMethod.Get))
   {
        <p> Reg. Number: @Html.TextBox("SearchString") <br />
         <input name="SearchString" type="submit" value="Search" class="btn btn-default"/></p>
    }
</p>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Vehicle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RegNr)
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Vehicle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RegNr)
        </td>

        <td>
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Check Out", "CheckOut", new { id=item.Id })
        </td>
    </tr>
}

</table>
